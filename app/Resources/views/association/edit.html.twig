{% block container %}

    <div class="container">
        <div class="row">


            <div class="col-lg-8 col-md-8 col-sm-12">
                <div class="content-inner-page">


                    <div class="section__content section__content--p30">
                        <div class="container">
                            <div class="container-fluid">
                                <div class="center-block">
                                    <div class="card">

                                        <div class="card-body">

                                            <hr>
                                            <div class="card-title">
                                                <h4 class="text title-2">Modifier </h4>
                                            </div>
                                            <hr>
                                            <div class="container-fluid">

                                                {{ form_start(edit_form) }}



                                                <div class="form-group">

                                                    {{ form_widget(edit_form.raisonSociale, { 'attr': {'class': 'form-control ' } }) }}

                                                </div>
                                                <div class="form-group">

                                                    {{ form_widget(edit_form.address, { 'attr': {'class': 'form-control ' } }) }}

                                                </div>
                                                <div class="form-group">
                                                    {{ form_widget(edit_form.telephone, { 'attr': {'class': 'form-control ' } }) }}

                                                </div>


                                                <div class="form-group">
                                                    {{ form_widget(edit_form.domaine, { 'attr': {'class': 'form-control ' } }) }}


                                                </div>
                                                <div class="form-group">
                                                    {{ form_widget(edit_form.gouvernorat, { 'attr': {'class': 'form-control ' } }) }}


                                                </div>
                                                <button  type="submit" value="Edit" id="payment-button"  class="btn btn-lg btn-info btn-block">
                                                    Soumettre
                                                    <span id="payment-button-sending" style="display:none;">Sending…</span>
                                                </button>
                                                {{ form_end(edit_form)}}
                                                <ul>
                                                    <li>
                                                        <a href="{{ path('association_index') }}">Retour à la liste </a>
                                                    </li>
                                                    <li>
                                                        {{ form_start(delete_form) }}
                                                        <input type="submit" value="Delete">
                                                        {{ form_end(delete_form) }}
                                                    </li>
                                                </ul>
                                            </div>
                                        </div>


                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
